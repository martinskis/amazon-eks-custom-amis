{
    "bridge": "none",
    "log-level": "info",
    "log-driver": "json-file",
    "log-opts": {
        "max-size": "10m",
        "max-file": "5"
    },
    "icc": false,
    "iptables": true,
    "storage-driver": "overlay2",
    "default-ulimits": {
      "nofile": {
          "Name": "nofile",
          "Hard": 200,
          "Soft": 100
        },
        "nofile": {
          "Name": "nproc",
          "Hard": 2048,
          "Soft": 1024
        }
    },
{% if 'enabled' in sestatus.stdout %}
    "selinux-enabled": true,
{% endif %}
    "live-restore": true,
    "userland-proxy": false,
    "max-concurrent-downloads": 10,
    "experimental": false,
    "insecure-registries": [],
    "no-new-privileges": true
}
